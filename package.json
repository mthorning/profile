{
  "name": "profile",
  "version": "1.2.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "serve": "moduleserve ./ --port 8000 --spa",
    "prod": "yarn clean && yarn run build && node_modules/.bin/webpack",
    "serve:prod": "moduleserve ./dist --port 9000 --spa",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Matt Thorning",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-icons": "^3.8.0",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^7.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "moduleserve": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn clean && yarn run build && git add ."
    }
  }
}
