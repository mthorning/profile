{
  "name": "profile",
  "version": "1.5.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "server": "moduleserve ./ --port 8000 --spa",
    "prod": "npm run clean && npm run build && node_modules/.bin/webpack",
    "server:prod": "moduleserve ./dist --port 9000 --spa",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Matt Thorning",
  "license": "MIT",
  "dependencies": {
    "@ahrefs/bs-recharts": "^0.3.3",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-icons": "^3.8.0",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "moduleserve": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean && npm run build && git add ."
    }
  }
}
